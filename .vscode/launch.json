    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "debug nest",
            "runtimeExecutable": "npm",
            "args": [
                "run",
                "start:dev",
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "console": "integratedTerminal",
        },
        {
            "type": "pwa-node",
            "request": "attach",
            "name": "Attach",
            "port":9229,
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
    ]
}